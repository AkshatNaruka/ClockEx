{
    "manifest_version": 2,
    "name": "Aesthetic Clock",
    "description": "This extension opens a new tab with a clock",
    "version": "1.0",
    "permissions": ["tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": "clock.jpeg"
    }
  }